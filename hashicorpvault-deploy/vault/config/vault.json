{                                    
  "listener":  {                     
    "tcp":  {                        
      "address":  "0.0.0.0:8200",  
      "tls_disable":  "false",
      "tls_cert_file": "/vault/config/cert.pem",
      "tls_key_file": "/vault/config/key.pem"
    }                                
  },                                 
  "backend": {                       
    "file": {                        
      "path": "/vault/data"          
    }                                
  },                                 
  "default_lease_ttl": "168h",       
  "max_lease_ttl": "0h",
  "api_addr": "https://0.0.0.0:8200",
  "ui": true
}  